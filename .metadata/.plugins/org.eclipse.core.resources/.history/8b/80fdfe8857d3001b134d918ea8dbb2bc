package FifthTry;

public class Student{
	int numCourses = 0;
	String[] courses = new String[] {"Math", "Biology", "Physics", "Chemist"};
	int[] grades = new int[3];
	
	public Student (int numCourses, String[] courses, int[] grades) {
		this.numCourses= 4;
		this.courses=courses;
		this.grades=grades;
		
	}
	
//	public setCourses(String[] courses) {
//		return courses;
//	}
	
	public void setGrades(int[] grades) {
		this.grades=grades;
	}
	public int setNumCourses() {
		return numCourses;
	}
	
	public void averageGrades(int avgGrades) {
		grades/numCourses;
	}
	
}
